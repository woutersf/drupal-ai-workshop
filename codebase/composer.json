{
    "name": "woutersf/drupal-ai-workshop",
    "description": "Project to test Drupal and AI.",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^2.0",
        "drupal/admin_toolbar": "^3.4",
        "drupal/ai": "^1.1",
        "drupal/ai_agents": "^1.1",
        "drupal/ai_api": "^1.0@dev",
        "drupal/ai_block": "^1.0",
        "drupal/ai_image": "1.0.x-dev@dev",
        "drupal/ai_image_alt_text": "^1.0",
        "drupal/ai_provider_litellm": "^1.1@beta",
        "drupal/ai_provider_mistral": "^1.0@beta",
        "drupal/ai_provider_openai": "^1.0",
        "drupal/ai_search_block": "^1.0@dev",
        "drupal/ai_vdb_provider_pinecone": "^1.0@beta",
        "drupal/ai_vdb_provider_postgres": "^1.0@alpha",
        "drupal/backup_migrate": "^5.1",
        "drupal/convertapi": "1.0.x-dev",
        "drupal/core-composer-scaffold": "^10",
        "drupal/core-project-message": "^10",
        "drupal/core-recommended": "^10",
        "drupal/ctools": "^4.0",
        "drupal/eca": "^2.1",
        "drupal/eca_content": "^2.1",
        "drupal/fireworksai": "^1.1@RC",
        "drupal/key": "^1.17",
        "drupal/search_api": "^1.29",
        "drupal/simple_crawler": "^1.0@beta",
        "drupal/token": "^1.15",
        "drupal/yoast_seo": "^2.2",
        "drush/drush": "^12",
        "justinrainbow/json-schema": "^6.4"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "cweagans/composer-patches": true,
            "zodiacmedia/drupal-libraries-installer": true,
            "php-http/discovery": true
        },
        "sort-packages": true
    },
    "extra": {
        "enable-patching": true,
        "patches": {
        "drupal/ai_image": {
          "fix_ckeditor_compatiblity": "https://www.drupal.org/files/issues/2025-08-01/incompatible-with-drupal-core-10.5-3539361-2.patch"
        }
    },
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    }
}
